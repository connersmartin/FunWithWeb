@model IEnumerable<SpotifyAPI.Web.Models.FullArtist>

    <div class="col-xs-4">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Artists</th>
                </tr>
             </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.ActionLink(item.Name, "Detail", new { id = item.Id })
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

